{
  "name": "website-benwhite",
  "version": "1.0.0",
  "description": "Website for benwhite.com.au",
  "author": "Ben White",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "11ty": "NODE_ENV=production npx @11ty/eleventy",
    "build": "npm-run-all 11ty tailwind",
    "local:11ty": "npx @11ty/eleventy --serve --quiet",
    "local:11ty:debug": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
    "local:11ty:benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "local:clean": "rimraf _site",
    "local:killports": "npx kill-port --port 8080",
    "local:tailwind": "npx tailwindcss -i src/style.css -o _site/style.css --watch",
    "start": "npm-run-all local:killports local:clean --parallel local:11ty local:tailwind",
    "tailwind": "NODE_ENV=production npx tailwindcss -i src/style.css -o _site/style.css -m",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@11ty/eleventy": "3.0.0-alpha.13",
    "@11ty/eleventy-plugin-bundle": "^2.0.2",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@formkit/tempo": "^0.1.1",
    "esbuild": "^0.21.5",
    "html-minifier-terser": "^7.2.0",
    "markdown-it": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "kill-port": "^2.0.1",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "rimraf": "^5.0.7"
  }
}
