<!doctype html>
<html
  lang="en"
  class="scroll-smooth">
  <head>
    <link
      webc:keep
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Inter:wght@500,700&family=JetBrains+Mono&display=swap" />
    <link
      webc:keep
      rel="stylesheet"
      href="/style.css" />
    <template
      webc:is="favicon-emoji"
      @emoji="âœ¨"></template>
    <script
      webc:keep
      @raw="getBundle('js')"></script>
  </head>
  <body
    class="flex min-h-screen w-full flex-col bg-stone-200 dark:bg-stone-900">
    <a
      href="#content"
      class="focus-state sr-only left-1/2 top-1.5 z-40 -translate-x-1/2 bg-stone-100 text-sm focus:not-sr-only focus:absolute focus:p-1.5 sm:top-2 dark:bg-stone-900">
      skip to content
    </a>
    <header
      class="wrapper padding-x width-site mt-6 flex flex-row justify-between text-sm">
      <nav-breadcrumbs></nav-breadcrumbs>
      <nav-menu></nav-menu>
    </header>
    <template
      webc:nokeep
      @html="content"></template>
    <footer
      class="wrapper padding-x width-site my-6 flex flex-row justify-between text-sm">
      <nav-social></nav-social>
      <partial-last-commit></partial-last-commit>
    </footer>
    <script
      webc:keep
      @raw="getBundle('js', 'defer')"
      defer></script>
  </body>
</html>
