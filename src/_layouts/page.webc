---
layout: base
---

<main
  id="content"
  class="wrapper padding-x padding-y width-reading flex-grow">
  <article>
    <header class="mb-10">
      <h1
        webc:if="title"
        @text="title"
        class="mb-1"></h1>
      <time-formatted :date="date"></time-formatted>
    </header>
    <template
      webc:nokeep
      @html="content"></template>
  </article>
</main>
